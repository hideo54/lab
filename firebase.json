{
  "hosting": {
    "site": "hideo-lab",
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
      {
        "source": "/api/music-separation",
        "run": {
          "serviceId": "lab-music-separation",
          "region": "asia-northeast1"
        }
      }
    ]
  }
}
